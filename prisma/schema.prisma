// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model User {
  id         Int    @id @default(autoincrement())
  uniqueCode String @unique
}

model Song {
  id         Int    @id @default(autoincrement())
  clientName String
  songTitle  String
  artist     String
  songUrl    String
  votes      Int
}
